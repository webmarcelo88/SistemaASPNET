@model TesteBludata.Model.FornecedorTelefoneModel
@{
    ViewBag.Title = "Deletar";
}
<h3 class="text-danger">Deseja excluir este telefone?</h3>
<div class="form-horizontal">
    <div class="form-group">
        @Html.LabelFor(m => m.Telefone, new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.Telefone, new { @class = "form-control", @disabled = "disabled" })
        </div>
    </div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Excluir" class="btn btn-danger" />
                @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-warning" })
            </div>
        </div>
    }
</div>